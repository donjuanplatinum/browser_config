{
   "background": {
      "service_worker": "background.js"
   },
   "content_scripts": [ {
      "js": [ "content-scripts/content.js" ],
      "matches": [ "*://www.douyin.com/*" ]
   } ],
   "description": "通过向视频右上角添加下载按钮，从而下载抖音无水印视频",
   "icons": {
      "128": "icon/128.png",
      "16": "icon/16.png",
      "32": "icon/32.png",
      "48": "icon/48.png",
      "96": "icon/96.png"
   },
   "key": "",
   "manifest_version": 3,
   "name": "抖音视频下载插件",
   "permissions": [ "downloads" ],
   "update_url": "https://clients2.google.com/service/update2/crx",
   "version": "1.0.0",
   "web_accessible_resources": [ {
      "matches": [ "*://www.douyin.com/*" ],
      "resources": [ "/assets/*.svg" ]
   } ]
}
